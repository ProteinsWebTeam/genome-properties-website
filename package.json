{
  "name": "genome-properties-website",
  "version": "1.0.0",
  "description": "Website for the Genome Properties project",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c && cp node_modules/genome-properties-viewer/bin/* dist/ && cp node_modules/genome-properties-viewer/*.css dist/",
    "watch": "nodemon -w src -x \"npm run build\"",
    "serve": "browser-sync start --server -f dist/**/*.*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gustavo Salazar <gsalazar@ebi.ac.uk>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.12",
    "nodemon": "^1.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "genome-properties-viewer": "git+https://github.com/ProteinsWebTeam/genome-properties-viewer.git",
    "showdown": "^1.7.1"
  }
}
