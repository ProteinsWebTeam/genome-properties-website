{
  "name": "genome-properties-website",
  "version": "1.0.0",
  "description": "Website for the Genome Properties project",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c && cp node_modules/genome-properties-viewer/bin/* dist",
    "rollup-watch": "rollup -cw",
    "browser-sync": "browser-sync start --server -f dist/**/*.*",
    "serve": "npm run rollup-watch & npm run browser-sync",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gustavo Salazar <gsalazar@ebi.ac.uk>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.12",
    "nodemon": "^1.11.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-commonjs": "^8.2.6"
  },
  "dependencies": {
    "genome-properties-viewer": "git+https://github.com/ProteinsWebTeam/genome-properties-viewer.git",
    "rst2mdown": "^0.1.0",
    "showdown": "^1.7.1"
  }
}
